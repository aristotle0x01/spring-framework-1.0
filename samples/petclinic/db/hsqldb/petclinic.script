CREATE TABLE OWNERS_SEQ(SEQ INTEGER NOT NULL IDENTITY PRIMARY KEY)
CREATE TABLE TYPES_SEQ(SEQ INTEGER NOT NULL IDENTITY PRIMARY KEY)
CREATE TABLE PETS_SEQ(SEQ INTEGER NOT NULL IDENTITY PRIMARY KEY)
CREATE TABLE VETS_SEQ(SEQ INTEGER NOT NULL IDENTITY PRIMARY KEY)
CREATE TABLE SPECIALTIES_SEQ(SEQ INTEGER NOT NULL IDENTITY PRIMARY KEY)
CREATE TABLE VISITS_SEQ(SEQ INTEGER NOT NULL IDENTITY PRIMARY KEY)
CREATE TABLE VETS(ID INTEGER(4) NOT NULL IDENTITY PRIMARY KEY,FIRST_NAME VARCHAR_IGNORECASE(30),LAST_NAME VARCHAR_IGNORECASE(30))
CREATE INDEX VETS_LAST_NAME ON VETS(LAST_NAME)
CREATE TABLE SPECIALTIES(ID INTEGER(4) NOT NULL IDENTITY PRIMARY KEY,NAME VARCHAR_IGNORECASE(80))
CREATE INDEX SPECIALTIES_NAME ON SPECIALTIES(NAME)
CREATE TABLE VET_SPECIALTIES(VET_ID INTEGER(4) NOT NULL,SPECIALTY_ID INTEGER(4) NOT NULL,CONSTRAINT FK_VET_SPECIALTIES_VETS FOREIGN KEY(VET_ID) REFERENCES VETS(ID),CONSTRAINT FK_VET_SPECIALTIES_SPECIALTIES FOREIGN KEY(SPECIALTY_ID) REFERENCES SPECIALTIES(ID))
CREATE TABLE TYPES(ID INTEGER(4) NOT NULL IDENTITY PRIMARY KEY,NAME VARCHAR_IGNORECASE(80))
CREATE INDEX TYPES_NAME ON TYPES(NAME)
CREATE TABLE OWNERS(ID INTEGER(4) NOT NULL IDENTITY PRIMARY KEY,FIRST_NAME VARCHAR_IGNORECASE(30),LAST_NAME VARCHAR_IGNORECASE(30),ADDRESS VARCHAR_IGNORECASE(255),CITY VARCHAR_IGNORECASE(80),TELEPHONE VARCHAR_IGNORECASE(20))
CREATE INDEX OWNERS_LAST_NAME ON OWNERS(LAST_NAME)
CREATE TABLE PETS(ID INTEGER(4) NOT NULL IDENTITY PRIMARY KEY,NAME VARCHAR_IGNORECASE(30),BIRTH_DATE DATE,TYPE_ID INTEGER(4) NOT NULL,OWNER_ID INTEGER(4) NOT NULL,CONSTRAINT FK_PETS_OWNERS FOREIGN KEY(OWNER_ID) REFERENCES OWNERS(ID),CONSTRAINT FK_PETS_TYPES FOREIGN KEY(TYPE_ID) REFERENCES TYPES(ID))
CREATE INDEX PETS_NAME ON PETS(NAME)
CREATE TABLE VISITS(ID INTEGER(4) NOT NULL IDENTITY PRIMARY KEY,PET_ID INTEGER(4) NOT NULL,VISIT_DATE DATE,DESCRIPTION VARCHAR_IGNORECASE(255),CONSTRAINT FK_VISITS_PETS FOREIGN KEY(PET_ID) REFERENCES PETS(ID))
CREATE INDEX VISITS_PET_ID ON VISITS(PET_ID)
